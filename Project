import pandas as pd
import matplotlib.pyplot as plt

# Read the dataset (replace 'dataset.csv' with your actual file path)
df = pd.read_csv('/content/superstore.csv', encoding="latin1")

# Convert Order_Date column to datetime format
df['Order_Date'] = pd.to_datetime(df['Order_Date'])

# Extract year from Order_Date
df['Year'] = df['Order_Date'].dt.year

# Group by Year and sum up the sales
yearly_sales = df.groupby('Year')['Sales'].sum()

# Find the top selling year
top_selling_year = yearly_sales.idxmax()

# Plotting
plt.figure(figsize=(10, 6))
yearly_sales.plot(kind='bar', color='skyblue')
plt.title('Top Selling Year')
plt.xlabel('Year')
plt.ylabel('Total Sales')
plt.xticks(rotation=45, ha='right')
plt.axvline(x=top_selling_year, color='red', linestyle='--', label='Top Selling Year: ' + str(top_selling_year))
plt.legend()
plt.tight_layout()
plt.show()

import pandas as pd
import matplotlib.pyplot as plt

# Read the dataset (replace '/content/superstore.csv' with your actual file path)
df = pd.read_csv('/content/superstore.csv', encoding="latin1")

# Group by City and count the number of products sold
city_product_count = df.groupby('City').size().sort_values()

# Plotting
plt.figure(figsize=(10, 6))
city_product_count.plot(kind='line', marker='o', color='skyblue')
plt.title('Number of Products Sold in Each City')
plt.xlabel('City')
plt.ylabel('Number of Products Sold')
plt.xticks(rotation=90)
plt.grid(True)
plt.tight_layout()
plt.show()

import pandas as pd
import matplotlib.pyplot as plt
df = pd.read_csv('/content/superstore.csv', encoding="latin1")

segment_sales = df.groupby('Segment')['Sales'].sum()


plt.figure(figsize=(10, 6))
plt.hist(df['Sales'], bins=20, color='skyblue', alpha=0.7)
plt.title('Segment-wise Selling Details')
plt.xlabel('Sales')
plt.ylabel('Frequency')
plt.grid(True)
plt.tight_layout()
plt.show()
